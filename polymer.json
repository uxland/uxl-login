{
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "entrypoint": "index.html",
  "sources": [
    "src/**/*.js",
    "node_modules/@uxland/**/*.*",
    "node_modules/@eug/**/*.*",
    "node_modules/@polymer/**/*.js",
    "node_modules/date-fns/esm/**/*.js",
    "node_modules/lit-html/**/*",
    "node_modules/ramda/es/**/*.js",
    "package.json"
  ],
  "builds": [
    {
      "name": "dev",
      "addServiceWorker": false,
      "js": {"minify": true, "compile": false},
      "css": {"minify": false},
      "html": {"minify": false},
      "bundle": false,
      "addPushManifest": false
    }
  ]
}
